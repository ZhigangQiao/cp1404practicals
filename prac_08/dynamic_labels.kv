BoxLayout:
    orientation: 'vertical'
    BoxLayout:
        orientation: 'vertical'
        # This layout will be populated with Label widgets in the app code
        id: labels_box
    BoxLayout:
        orientation: 'horizontal'
        size_hint_y: 0.1
        Label:
            text: "Dynamic Labels App"
        Button:
            text: "Exit"
            on_press: app.stop()
